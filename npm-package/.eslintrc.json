{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": "airbnb-base",
    "rules": {
        "object-curly-spacing": 0,
        "object-curly-newline": 0,
        "no-case-declarations": 0,
        "import/prefer-default-export": "off"
    }
}
